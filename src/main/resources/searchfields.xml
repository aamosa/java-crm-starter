<?xml version="1.0" encoding="UTF-8"?>
<config>
    <select label="Display All"></select>
    <select label="Contact Name" entityClass="contact">
        <field type="text" label="First Name" value="fName" default="true"></field>
        <field type="text" label="Last Name" value="lName" default="true"></field>
    </select>

    <select label="User Name" entityClass="user">
        <field type="text" label="User Name" value="userName" default="true"></field>
    </select>

    <select label="Id">
        <field type="number" label="Id" value="id"></field>
    </select>

    <select label="Created Date">
        <field type="date" label="From Date" value="searchDateFrom"></field>
        <field type="date" label="To Date" value="searchDateTo"></field>
    </select>

    <select label="Created By" entityClass="task">
        <field type="text" label="Created By User Name" value="userName" default="true"></field>
    </select>

    <select label="Assigned To" entityClass="task">
        <field type="text" label="Assigned to User Name" value="assgnUserName"></field>
    </select>
</config>